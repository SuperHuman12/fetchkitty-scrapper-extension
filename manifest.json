{
  "manifest_version": 3,
  "name": "Fetch Kitty AI Scraper",
  "version": "1.0.1",
  "description": "AI-powered tool that extracts, understands, and cleans web content using AI for efficient content curation. Just Purrrfect !",

  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "action": {
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    },
    "default_title": "Fetch Kitty AI Scrapper"
  },
  "permissions": [
    "activeTab",
    "scripting",
    "tabs"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content-script.js"]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": ["extract-images.js"],
      "matches": ["<all_urls>"]
    }
  ]
}